@model FrontEnd.Models.RolViewModel

@{
    ViewData["Title"] = "Detalles del rol";
}

<h1>Detalles del rol</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Id
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdRol)
        </dd>
        <dt class = "col-sm-2">
            Nombre
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NombreRol)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Editar", "Edit", new {  id = Model.IdRol  }) |
    <a asp-action="Index">Regresar</a>
</div>
